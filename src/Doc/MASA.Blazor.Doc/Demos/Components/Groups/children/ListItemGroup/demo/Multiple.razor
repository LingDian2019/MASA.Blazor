<MCard MaxWidth="400">
    <MList>
        <MListItemGroup @bind-Values="@selected" Multiple Color="indigo">
            @foreach (var item in _items)
            {
                <MListItem Value="@item.Text">
                    <MListItemIcon>
                        <MIcon>@item.Icon</MIcon>
                    </MListItemIcon>
                    <MListItemContent>
                        <MListItemTitle>@item.Text</MListItemTitle>
                    </MListItemContent>
                </MListItem>
            }
        </MListItemGroup>
    </MList>
</MCard>

@code {

    public class Item
    {
        public string Icon { get; set; }
        public string Text { get; set; }
    }

    static readonly Item[] _items =
    {
        new()
        {
            Icon = "mdi-inbox",
            Text = "Inbox"
        },
        new()
        {
            Icon = "mdi-star",
            Text = "Star"
        },
        new()
        {
            Icon = "mdi-send",
            Text = "Send"
        },
        new()
        {
            Icon = "mdi-email-open",
            Text = "Drafts"
        }
    };

    List<StringNumber> selected = _items.Where(u => u.Text == "Star").Select(u => (StringNumber)u.Text).ToList();
}